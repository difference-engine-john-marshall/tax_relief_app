<div class="container-fluid">
  <div class="row faq-form-top-title">
    <h1>Edit an FAQ</h1>
  </div>
</div>

<%= form_tag "/faq/#{@faq.id}", method: :patch do %>  

  <%= render "faq_form" %>
  
  <div class="row">
    <div class="button-submit">
      <%= submit_tag "Update FAQ", class: "btn btn-default btn-lg faq-form-button-submit" %>
    </div>
  </div>

<% end %>



