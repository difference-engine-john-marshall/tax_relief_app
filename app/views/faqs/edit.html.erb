<%= form_tag "/dashboard/faqs/#{@faq.id}", method: :patch do %>  
<div class="container-fluid">
  <div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" id="faq-text">
      <div class="panel panel-default faq-form-panel" id="faq-text-panel">
        <div class="panel-heading" id="faq-form-heading">
          <h3 class="panel-title faq-form-title">Edit FAQ Question</h3>
        </div>

        <%= render "faq_form" %>

      </div>
    </div>
  </div>
</div>

<div class="container-fluid">
  <div class="row">
    <div class="button-submit">
      <%= submit_tag "Update FAQ", class: "btn btn-default btn-lg faq-form-button-submit" %>
    </div>
  </div>
</div>

<% end %>



