<% @faqs.each do |faq| %>

  <div class="row" id="new-title">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" id="question-text">
      <div class="panel panel-default" id="question-text-panel">
        <div class="panel-heading" id="question-text-panel-heading">
          <h3 class="panel-title">FAQ Text</h3>
        </div>
        <div class="panel-body textbox-border" id="question-text-body">       
            <%= raw faq.question_text %>
        </div>

        <div class="panel-body textbox-border" id="question-text-body">       
            <%= raw faq.answer_text %>
        </div>
        <a href="/dashboard/faqs/<%= faq.id %>/edit" class="btn btn-default" id="button-edit" role="button">Edit FAQ</a>
        <a href="/dashboard/faqs/<%= faq.id %>" class="btn btn-default" role="button" id="button-delete" data-method="delete">Delete FAQ</a>
      </div>
    </div>
  </div>
<% end %>
