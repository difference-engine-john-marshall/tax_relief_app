<% @contents.each do |content| %>

  <div class="row" id="new-title">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12" id="question-text">
      <div class="panel panel-default" id="question-text-panel">
        <div class="panel-heading" id="question-text-panel-heading">
          <h3 class="panel-title"><%= content.content_type.capitalize %> Text</h3>
        </div>
        <div class="panel-body textbox-border" id="question-text-body">       
            <%= raw content.text %>
        </div>
        <a href="/dashboard/contents/<%= content.id %>/edit" class="btn btn-default" id="button-edit" role="button">Edit Text</a>
      </div>
    </div>
  </div>
<% end %>
